{
    "name": "thadafinser/user-agent-parser",
    
    "description": "UserAgentParser abstraction for different parse providers",
    "keywords": [
        "UA",
        "UA parser",
        "UAparser",
        "User agent",
        "User agent parser",
        "User agent detection",
        "UserAgent", 
        "UserAgentParser",
        
        "Parser", 
        "Detection",
        
        "Browser",
        "Engine",
        "Rendering engine",
        "OS",
        "Operating system",
        "Device",
        "device detection",
        "mobile detection",
        "mobile detector",
        "mobile device detection",
        "bot detection", 
        
        "get_browser",
        "browscap",
        "piwik",
        "sinergi",
        "whichbrowser",
        "woothee",
        "wurfl",
        
        "udger",
        "useragentapi",
        "useragentstring",
        "whatismybrowser"
    ],
    
    "license": "MIT",
    "authors": [
        {
            "name": "ThaDafinser",
            "email": "martin.keckeis1@gmail.com"
        }
    ],
    
    "autoload": {
        "psr-4": {
            "UserAgentParser\\":  "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "UserAgentParserTest\\Unit\\":  "tests/unit",
            "UserAgentParserTest\\Integration\\":  "tests/integration"
        }
    },
    
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "^6.1"
    },
    
    "suggest": {
        "browscap/browscap-php": "Needed to use Provider\\BrowscapPhp",
        "donatj/phpuseragentparser": "Needed to use Provider\\DonatjUAParser",
        "piwik/device-detector": "Needed to use Provider\\PiwikDeviceDetector",
        "sinergi/browser-detector": "Needed to use Provider\\SinergiBrowserDetector",
        "ua-parser/uap-php": "Needed to use Provider\\UAParser",
        "whichbrowser/parser": "Needed to use Provider\\WhichBrowser",
        "woothee/woothee": "Needed to use Provider\\Woothee",
        "mimmi20/wurfl": "Needed to use Provider\\Wurfl",
        "zsxsoft/php-useragent": "Needed to use Provider\\Zsxsoft"
    },
    
    "require-dev": {
        "phpunit/PHPUnit": "^4.0|^5.0",
        "fabpot/php-cs-fixer": "^1.11",
        
        "browscap/browscap-php": "dev-master",
        "donatj/phpuseragentparser": "^0.5.0",
        "piwik/device-detector": "^3.5",
        "sinergi/browser-detector": "^5.1",
        "ua-parser/uap-php": ">=3.4.3,<4.0",
        "whichbrowser/parser": ">=2.0.10,<3.0",
        "woothee/woothee": "^1.2",
        "mimmi20/wurfl": "^1.6",
        "zsxsoft/php-useragent": "^1.0"
    }
}
